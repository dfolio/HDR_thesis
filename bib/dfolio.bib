@Article{2014_isj_tao,
  Title                    = {Robotized Tele-Echography: an Assisting Visibility Tool to Support Expert Diagnostic},
  Author                   = {Alexandre Krupa and \DavidFolio and Cyril Novales and Pierre Vieyres and Tao Li},
