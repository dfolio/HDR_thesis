\newcommand{\NewSymb}[5][]{
  % \cxset{sort/.store in=\sym@sort@cx,}
  % \cxset{sort=#2,}
  % \cxset{#1}
  \expandafter\gdef\csname #2\endcsname{\gls{#2}\xspace}
  %\expandafter\gdef\l\csname #2\endcsname{\glslink{#2}{#2}\xspace}
  \ifx#4\@empty
  \newglossaryentry{#2}{type=nomenclature,name=\glslink{#2}{#3},symbol={--},description={#5}, #1}
  \else
  \newglossaryentry{#2}{type=nomenclature,name=\glslink{#2}{#3},symbol=[\,#4\,],description={#5}, #1}
  \fi
}

